universe = vanilla
executable = /opt/PepPrograms/fasttree_linux
arguments = "-nt -gamma -slow -boot 10 dummy_snps.fasta"

output = tree.out
error = fasttree.err
log = fasttree.log

transfer_executable = FALSE
should_transfer_files = YES
when_to_transfer_output = ON_EXIT
transfer_input_files = dummy_snps.fasta

request_cpus = 1
request_memory = 50MB
request_disk = 1MB

queue
